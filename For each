Apex code for For each
---------------------------------------------------------------------------------------------------------------------------
public class foreach 
{
    public static void main()
    {
        set<string> name = new set<string>();
        name.add('Chennai');
        name.add('Mumbai');
        name.add('Bangalore');
        name.add('Gujarat');
        
        for(string na : name)
        {
            system.debug(na);
        }
    }
    
}
--------------------------------------------------------------------------------------------------------------------------
# To Run the code #
foreach.main();
