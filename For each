Apex code for For each
---------------------------------------------------------------------------------------------------------------------------
public class foreach 
{
    public static void main()
    {
        set<string> name = new set<string>();
        name.add('Dhoni');
        name.add('Sachin');
        name.add('Yuvraj');
        name.add('Ganguly');
        name.add('Raina');
        
        for(string na : name)
        {
            system.debug(na);
        }
    }
    
}
--------------------------------------------------------------------------------------------------------------------------
# To Run the code #
foreach.main()
