public class Datatypes {
    public static void main()
    {
        string Academy = 'YOUR FUTURE';
        system.debug(Academy);
        
        string Role = 'Professor';
        system.debug(Role);
        
        string Name = 'Prasanth';
        system.debug(Name);
        
        string Qualification = 'MCA';
        system.debug(Qualification);
        
        boolean ClassStarted = true;
        system.debug(ClassStarted);
        
        Integer students = 30;
        system.debug(students);
        
        Long fees = 40000L;
        system.debug(fees);
        
        Decimal hours = 3.30;
        system.debug(hours);
        
        double batch = 30/2;
        system.debug(batch);
        
    }
    
    
}
